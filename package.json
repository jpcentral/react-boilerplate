{
  "name": "zazou-pottery",
  "version": "1.0.0",
  "description": "Pottery Studio website for Jacou Vettard",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "build": "webpack",
    "go": "run-p dev:client dev:server",
